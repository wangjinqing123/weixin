.section {
  height: 100%;
  box-sizing: border-box; }
  .section .userInfo {
    padding-bottom: 6px; }
    .section .userInfo .item {
      box-sizing: border-box; }
      .section .userInfo .item.head {
        height: 120rpx;
        display: flex;
        padding: 18rpx 12rpx;
        box-sizing: border-box; }
        .section .userInfo .item.head view {
          flex: 1; }
          .section .userInfo .item.head view.headImgBox {
            align-self: center;
            position: relative; }
            .section .userInfo .item.head view.headImgBox image {
              border-radius: 50%;
              height: 84rpx;
              width: 84rpx;
              position: absolute;
              left: 0;
              top: 0;
              bottom: 0;
              right: 0;
              margin: auto; }
          .section .userInfo .item.head view.edit {
            box-sizing: border-box;
            font-size: 12px;
            padding-top: 20rpx; }
            .section .userInfo .item.head view.edit div {
              float: right;
              border-radius: 10px;
              box-sizing: border-box;
              width: 160rpx;
              display: flex; }
              .section .userInfo .item.head view.edit div image {
                height: 40rpx;
                width: 40rpx;
                border-radius: 50%; }
              .section .userInfo .item.head view.edit div text {
                transform: scale(0.8);
                line-height: 40rpx;
                flex: 1;
                text-align: right; }
      .section .userInfo .item.userName {
        text-align: center;
        height: 60rpx;
        line-height: 60rpx; }
      .section .userInfo .item.userID {
        line-height: 40rpx;
        height: 40rpx;
        text-align: center;
        font-size: 24rpx; }
      .section .userInfo .item.biaoqian {
        line-height: 40rpx;
        height: 40rpx;
        text-align: center;
        font-size: 24rpx; }
      .section .userInfo .item.guanzhufensi {
        height: 80rpx;
        display: flex;
        text-align: center; }
        .section .userInfo .item.guanzhufensi .middle {
          height: 50%;
          margin-top: 20rpx; }
        .section .userInfo .item.guanzhufensi .one {
          font-size: 24rpx;
          flex: 1;
          display: flex;
          flex-direction: column; }
          .section .userInfo .item.guanzhufensi .one text {
            transform: scale(0.98, 0.98);
            flex: 1; }
        .section .userInfo .item.guanzhufensi .two {
          font-size: 24rpx;
          flex: 1;
          display: flex;
          flex-direction: column; }
          .section .userInfo .item.guanzhufensi .two text {
            transform: scale(0.98, 0.98);
            flex: 1; }
      .section .userInfo .item.qiehuan {
        width: 100%;
        height: 50rpx;
        border-radius: 3px;
        display: flex;
        box-sizing: border-box; }
        .section .userInfo .item.qiehuan .inner-item {
          flex: 1;
          text-align: center;
          position: relative; }
          .section .userInfo .item.qiehuan .inner-item image {
            height: 12px;
            width: 12px;
            position: absolute;
            right: 16rpx;
            top: 10rpx; }
          .section .userInfo .item.qiehuan .inner-item text {
            font-size: 24rpx;
            line-height: 24rpx; }
    .section .userInfo .box {
      width: 100%;
      padding: 0 10px;
      box-sizing: border-box; }
