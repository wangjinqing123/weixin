.container-box {
  width: 100%;
  display: flex;
  flex-direction: column;
  height: 100%;
  box-sizing: border-box; }

.top-line {
  position: fixed;
  top: 0;
  width: 100%;
  height: 0rpx;
  border-top: 1rpx solid #ddd;
  z-index: 999 !important; }

.bottom-line {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 0rpx;
  border-top: 1rpx solid #ddd;
  z-index: 999 !important; }

